{
  "name": "grid-generator",
  "version": "1.0.0",
  "description": "CSS Grid Generator with 11ty",
  "scripts": {
    "clean": "rm -rf _site",
    "dev": "npm run clean ; concurrently -n \"11ty,vite\" -c \"blue,green\" \"npm run dev:eleventy\" \"npm run dev:vite\"",
    "dev:eleventy": "eleventy --serve --port=8080",
    "dev:vite": "vite build --watch",
    "build": "eleventy && vite build"
  },
  "type": "module",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@preact/preset-vite": "^2.10.2",
    "concurrently": "^9.2.1",
    "dedent": "^1.7.0",
    "prismjs": "^1.30.0",
    "qs": "^6.14.0",
    "refractor": "^5.0.0",
    "urlon": "^3.1.0",
    "vite": "^7.1.6",
    "water.css": "^2.1.1"
  },
  "dependencies": {
    "preact": "^10.27.2"
  }
}
